# Tokens das páginas (formato: page_id:token,page_id2:token2,...)
FB_PAGE_TOKENS="token"

# Webhook do Slack para envio de alertas
SLACK_WEBHOOK_URL=YOUR_WEBHOOK_HERE

# Credenciais do app do Facebook (necessárias para validar tokens com /debug_token)
FB_APP_ID=
FB_APP_SECRET=

# Versão da Graph API (opcional, padrão v19.0)
GRAPH_API_VERSION=v19.0

# Outros IDs de apps auxiliares (se precisar)
CHATBOT_APP_ID=
